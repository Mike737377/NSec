@model NSec.Model.SecurityEvent[]
<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Event</th>
            <th>IP</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var row in Model)
        {
            <tr>
                <td>@row.Date</td>
                <td>@row.EventType</td>
                <td>@row.AttackerProfile.IPAddress</td>
            </tr>
        }
    </tbody>
</table>
